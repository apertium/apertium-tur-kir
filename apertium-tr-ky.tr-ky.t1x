  <section-def-macros>
    <def-macro n="f_conv_case1" npar="1">
      <let><var n="c_post"/><lit v=""/></let>
      <choose>
        <when>
          <test>
            <equal><clip pos="1" side="sl" part="case"/><lit v=""/></equal>
          </test>
          <let><var n="c_case"/><lit-tag v="nom"/></let>
        </when>
        <when>
          <test>
            <equal><clip pos="1" side="sl" part="case"/><lit-tag v="ins"/></equal>
          </test>
          <let><var n="c_case"/><lit-tag v="nom"/></let>
          <let><var n="c_post"/><concat><lit v=" ^менен"/><lit-tag v="post"/><lit v="$"/></concat></let>
        </when>
        <otherwise>
          <let><var n="c_case"/><clip pos="1" side="sl" part="case"/></let>
        </otherwise>
      </choose>
    </def-macro>  

    <def-macro n="f_conv_tens1" npar="1">
      <choose>
        <when>
          <test>
            <equal><clip pos="1" side="tl" part="tens"/><lit-tag v="t_past"/></equal>
          </test>
          <let><var n="c_tens"/><lit-tag v="ifi"/></let>
        </when>
        <when>
          <test>
            <or>
              <equal><clip pos="1" side="tl" part="tens"/><lit-tag v="t_aor"/></equal>
              <equal><clip pos="1" side="tl" part="tens"/><lit-tag v="t_cont"/></equal>
            </or>
          </test>
          <let><var n="c_tens"/><lit-tag v="aor"/></let>
        </when>

        <otherwise>
          <let><var n="c_tens"/><clip pos="1" side="tl" part="tens"/></let>
        </otherwise>
      </choose>
    </def-macro>  


    <def-macro n="f_conv_num1" npar="1">
      <choose>
        <when>
          <test>
            <equal><clip pos="1" side="tl" part="nbr"/><lit-tag v="ND"/></equal>
          </test>
          <let><var n="c_num"/><lit-tag v="sg"/></let>
        </when>
        <otherwise>
          <let><var n="c_num"/><clip pos="1" side="tl" part="nbr"/></let>
        </otherwise>
      </choose>
    </def-macro>  


    <def-macro n="f_conv_poss1" npar="1">
      <choose>
        <when>
          <test>
            <or>
              <equal><clip pos="1" side="sl" part="poss"/><lit-tag v="1s"/></equal>
              <equal><clip pos="1" side="sl" part="poss"/><lit-tag v="p1s"/></equal>
            </or>
          </test>
          <let><var n="c_poss"/><lit-tag v="px1sg"/></let>
        </when>
        <when>
          <test>
            <or>
              <equal><clip pos="1" side="sl" part="poss"/><lit-tag v="2s"/></equal>
              <equal><clip pos="1" side="sl" part="poss"/><lit-tag v="p2s"/></equal>
            </or>
          </test>
          <let><var n="c_poss"/><lit-tag v="px2sg"/></let>
        </when>
        <when>
          <test>
            <or>
              <equal><clip pos="1" side="sl" part="poss"/><lit-tag v="3s"/></equal>
              <equal><clip pos="1" side="sl" part="poss"/><lit-tag v="p3s"/></equal>
            </or>
          </test>
          <let><var n="c_poss"/><lit-tag v="px3sg"/></let>
        </when>
        <when>
          <test>
            <or>
              <equal><clip pos="1" side="sl" part="poss"/><lit-tag v="1p"/></equal>
              <equal><clip pos="1" side="sl" part="poss"/><lit-tag v="p1p"/></equal>
            </or>
          </test>
          <let><var n="c_poss"/><lit-tag v="px1pl"/></let>
        </when>
        <when>
          <test>
            <or>
              <equal><clip pos="1" side="sl" part="poss"/><lit-tag v="2p"/></equal>
              <equal><clip pos="1" side="sl" part="poss"/><lit-tag v="p2p"/></equal>
            </or>
          </test>
          <let><var n="c_poss"/><lit-tag v="px2pl"/></let>
        </when>
        <when>
          <test>
            <or>
              <equal><clip pos="1" side="sl" part="poss"/><lit-tag v="3p"/></equal>
              <equal><clip pos="1" side="sl" part="poss"/><lit-tag v="p3p"/></equal>
            </or>
          </test>
          <let><var n="c_poss"/><lit-tag v="px3pl"/></let>
        </when>


        <otherwise>
          <let><var n="c_poss"/><lit v=""/></let>
        </otherwise>
      </choose>
    </def-macro>  

    <def-macro n="f_conv_pers1" npar="1">
      <choose>
        <when>
          <test>
            <equal><clip pos="1" side="sl" part="poss"/><lit-tag v="1s"/></equal>
          </test>
          <let><var n="c_pers"/><lit-tag v="p1.sg"/></let>
        </when>
        <when>
          <test>
            <equal><clip pos="1" side="sl" part="poss"/><lit-tag v="2s"/></equal>
          </test>
          <let><var n="c_pers"/><lit-tag v="p2.sg"/></let>
        </when>
        <when>
          <test>
            <equal><clip pos="1" side="sl" part="poss"/><lit-tag v="3s"/></equal>
          </test>
          <let><var n="c_pers"/><lit-tag v="p3.sg"/></let>
        </when>
        <when>
          <test>
            <equal><clip pos="1" side="sl" part="poss"/><lit-tag v="1p"/></equal>
          </test>
          <let><var n="c_pers"/><lit-tag v="p1.pl"/></let>
        </when>
        <when>
          <test>
            <equal><clip pos="1" side="sl" part="poss"/><lit-tag v="2p"/></equal>
          </test>
          <let><var n="c_pers"/><lit-tag v="p2.pl"/></let>
        </when>
        <when>
          <test>
            <equal><clip pos="1" side="sl" part="poss"/><lit-tag v="3p"/></equal>
