      </pattern>
      <action>
        <call-macro n="f_conv_case1">
          <with-param pos="1"/>
        </call-macro>
        <out>
          <lu>
            <clip pos="1" side="tl" part="lemh"/>
            <clip pos="1" side="tl" part="a_num"/>
            <var n="c_case"/>
            <clip pos="1" side="tl" part="lemq"/>
          </lu>
          <var n="c_post"/>
        </out>
      </action>
    </rule>
    
